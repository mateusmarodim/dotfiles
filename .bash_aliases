alias config='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
alias ls='ls -l --color --group-directories-first'
alias la='ls -a'
export WISEML=$HOME/Documents/lapti/wiseml
alias back="cd $WISEML/jvwiseml"
alias front="cd $WISEML/wisemlreact"
alias datasets="cd $WISEML/datasets"
alias edaliases="vim $HOME/.bash_aliases"
alias edbashrc="vim $HOME/.bashrc"
alias upbashrc="source $HOME/.bashrc"
alias vim="/usr/bin/nvim"
alias cl="clear"
